{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist && npx --yes prisma generate && tsc -p tsconfig.json",
    "dev": "dotenvx run -f .env.dev -- tsx --watch src/index.ts",
    "lint": "eslint .",
    "format": "prettier -c \"src/**/*.ts",
    "start:prod": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.5.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.76",
    "@swc/helpers": "^0.5.1",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^2.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@duckdb/node-api": "1.3.2-alpha.26",
    "@fastify/cors": "^10.0.0",
    "fastify": "^5.0.0",
    "fastify-type-provider-zod": "^4.0.0",
    "zod": "^3.23.8"
  }
}
